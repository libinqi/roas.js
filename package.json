{
  "name": "roas",
  "version": "1.0.1",
  "description": "A Web App like Ruby on Rails with Koa2, ES6 and Sequelize.",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/nodemon index.js",
    "test": "./node_modules/.bin/mocha test/bootstrap.test.js test/**/*.test.js  --opts test/mocha.opts",
    "build": "rd/s/q dist & babel src --out-dir dist --copy-files",
    "pm2": "NODE_ENV=production ./node_modules/.bin/pm2 start index.js -i 2 --name \"roas\" --max-memory-restart 1024M --merge-logs --log-date-format=\"YYYY-MM-DD HH:mm Z\" --output=\"log/production.log\"",
    "pm2:restart": "NODE_ENV=production ./node_modules/.bin/pm2 restart \"roas\"",
    "pm2:stop": "NODE_ENV=production ./node_modules/.bin/pm2 stop \"roas\"",
    "pm2:monit": "NODE_ENV=production ./node_modules/.bin/pm2 monit \"roas\"",
    "pm2:logs": "NODE_ENV=production ./node_modules/.bin/pm2 logs \"roas\"",
    "lint": "eslint . --ext .js",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "create:migration": "./node_modules/.bin/sequelize migration:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libinqi/roas.js.git"
  },
  "keywords": [
    "koa2",
    "orm",
    "babel",
    "es2015"
  ],
  "author": "libinqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libinqi/roas.js.git/issues"
  },
  "engines": {
    "node": ">= 5"
  },
  "homepage": "https://github.com/libinqi/roas.js.git#readme",
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.3.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.3.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.17.0",
    "moment": "^2.16.0",
    "mysql": "^2.12.0",
    "node-cache": "^4.1.0",
    "roas-mount": "^1.0.0",
    "roas-restql": "^1.0.0",
    "sequelize": "^3.25.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-polyfill": "^6.1.4",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "request": "^2.71.0",
    "sequelize-cli": "^2.4.0",
    "should": "^8.3.0",
    "supertest": "^1.2.0"
  }
}
