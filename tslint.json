{
    "rules": {
        "class-name": true,
        "curly": true,
        "indent": [
            true,
            "spaces"
        ],
        "max-line-length": [
            true,
            120
        ],
        "no-require-imports": false,
        "no-string-literal": false,
        "no-switch-case-fall-through": true,
        "no-unreachable": true,
        "no-unused-variable": true,
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": true,
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "whitespace": [
            true,
            "check-branch"
        ]
    }
}